{"ast":null,"code":"export default {\n  props: ['title'],\n  data() {\n    return {\n      steps: [{\n        name: 'Cart',\n        class: ''\n      }, {\n        name: 'Order',\n        class: ''\n      }, {\n        name: 'Checkout',\n        class: ''\n      }],\n      progressWidth: '0%'\n    };\n  },\n  computed: {\n    currentStepIndex() {\n      if (this.title === 'Cart') return 0;\n      if (this.title === 'Order') return 1;\n      if (this.title === 'Checkout') return 2;\n      return -1; // Handle default case if needed\n    }\n  },\n  methods: {\n    barStatus() {\n      this.steps.forEach((step, index) => {\n        if (index < this.currentStepIndex) {\n          step.class = 'bi bi-check-circle-fill text-primary';\n        } else if (index === this.currentStepIndex) {\n          if (index === 0) {\n            step.class = 'bi bi-1-circle text-primary';\n          } else if (index === 1) {\n            step.class = 'bi bi-2-circle text-primary';\n          } else if (index === 2) {\n            step.class = 'bi bi-3-circle text-primary';\n          }\n        } else {\n          step.class = 'bi bi-check-circle-fill text-black-50';\n        }\n      });\n      this.progressWidth = `${this.currentStepIndex * 50}%`;\n    }\n  },\n  created() {\n    this.barStatus();\n  }\n};","map":{"version":3,"names":["props","data","steps","name","class","progressWidth","computed","currentStepIndex","title","methods","barStatus","forEach","step","index","created"],"sources":["C:\\Users\\augus\\OneDrive\\桌面\\暫存區\\vue-practice\\brite\\src\\components\\StepsBar.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-lg-8 offset-lg-2 col-10 offset-1\">\n        <div class=\"position-relative\">\n          <div class=\"d-flex justify-content-between\">\n            <div v-for=\"(step, index) in steps\" :key=\"index\"\n                 class=\"d-flex flex-column w-25 align-items-center\"\n                 >\n        