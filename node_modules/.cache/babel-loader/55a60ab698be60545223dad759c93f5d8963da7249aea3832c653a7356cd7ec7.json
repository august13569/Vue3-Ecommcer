{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Navbar from '@/components/Navbar.vue';\nimport Footer from '@/components/Footer.vue';\nimport ProductList from '@/components/HorizontalProductList.vue';\nexport default {\n  components: {\n    Navbar,\n    Footer,\n    ProductList\n  },\n  data() {\n    return {\n      showAd: false,\n      isLoading: false,\n      allProducts: [],\n      allProductTypes: [],\n      pageInfo: {\n        title: 'Latest',\n        type: 'All',\n        name: ''\n      }\n    };\n  },\n  methods: {\n    getProducts() {\n      const url = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/products/all`;\n      this.isLoading = true;\n      this.$http.get(url).then(res => {\n        this.allProducts = res.data.products;\n        this.allProductTypes = ['All', ...new Set(this.allProducts.map(product => product.category))];\n        this.isLoading = false;\n      });\n    },\n    closeAd() {\n      this.showAd = false;\n    },\n    intoSelectedType(type) {\n      // 導航到 Products.vue 並將字串作為參數傳遞\n      this.$router.push({\n        path: '/products',\n        query: {\n          data: type\n        }\n      });\n    }\n  },\n  // mounted() {\n  //   setTimeout(() => {\n  //     this.showAd = true;\n  //   }, 3000);\n  // },\n  created() {\n    this.getProducts();\n  }\n};","map":{"version":3,"names":["Navbar","Footer","ProductList","components","data","showAd","isLoading","allProducts","allProductTypes","pageInfo","title","type","name","methods","getProducts","url","process","env","VUE_APP_API","VUE_APP_PATH","$http","get","then","res","products","Set","map","product","category","closeAd","intoSelectedType","$router","push","path","query","created"],"sources":["C:\\Users\\augus\\OneDrive\\桌面\\暫存區\\vue-practice\\brite\\src\\views\\front\\HomePage.vue"],"sourcesContent":["<!-- 文字內容尚未修改、圖片之後再選適合的、未來要加動畫效果 -->\n<template>\n  <Loading :active=\"isLoading\"></Loading>\n  <!-- Pop Ad of coupon code -->\n  <div class=\"d-flex w-100 h-100 position-fixed\"\n       style=\"background-color: rgba(0, 0, 0, 0.5); z-index: 9999;\"\n       v-if=\"showAd\"\n       @click=\"closeAd\"\n       >\n    <div class=\"d-flex m-auto pe-4\">\n      <button class=\"btn-close\" type=\"button\" @click=\"closeAd\"></button>\n      <img class=\"rounded w-100 h-100\" src=\"~@/assets/img/adpost.jpg\" alt=\"Advertisement\">\n    </div>\n  </div>\n  <Navbar></Navbar>\n  <!-- Carousel Banner -->\n  <header class=\"mb-3\">\n    <div id=\"carouselExampleIndicators\"\n         class=\"carousel slide position-relative\"\n         data-bs-ride=\"carousel\"\n         touch-action=\"pan-y\"\n         >\n      <div class=\"carousel-indicators\">\n        <button type=\"button\"\n                data-bs-target=\"#carouselExampleIndicators\"\n                data-bs-slide-to=\"0\"\n                class=\"active\"\n                aria-current=\"true\"\n                aria-label=\"Slide 1\"\n                ></button>\n        <button type=\"button\"\n                data-bs-target=\"#carouselExampleIndicators\"\n                data-bs-slide-to=\"1\"\n                aria-label=\"Slide 2\"\n                ></button>\n        <button type=\"button\"\n                data-bs-target=\"#carouselExampleIndicators\"\n                data-bs-slide-to=\"2\"\n                aria-label=\"Slide 3\"\n                ></button>\n        <button type=\"button\"\n                data-bs-target=\"#carouselExampleIndicators\"\n                data-bs-slide-to=\"3\"\n                aria-label=\"Slide 4\"\n                ></button>\n      </div>\n      <div class=\"carousel-inner\">\n        <div class=\"carousel-item active\">\n          <img src=\"~@/assets/img/carousel/carousel01.jpg\" class=\"d-block w-100\" alt=\"\"\n               style=\"max-height: 672px; object-fit: cover;\">\n        </div>\n        <div class=\"carousel-item\">\n          <img src=\"~@/assets/img/carousel/carousel02.jpg\" class=\"d-block w-100\" alt=\"\"\n               style=\"max-height: 672px; object-fit: cover;\">\n        </div>\n        <div class=\"carousel-item\">\n          <img src=\"~@/assets/img/carousel/carousel03.jpg\" class=\"d-block w-100\" alt=\"\"\n               style=\"max-height: 672px; object-fit: cover;\">\n        </div>\n        <div class=\"carousel-item\">\n          <img src=\"~@/assets/img/carousel/carousel04.jpg\" class=\"d-block w-100\" alt=\"\"\n               style=\"max-height: 672px; object-fit: cover;\">\n        </div>\n      </div>\n      <button class=\"carousel-control-prev\"\n              type=\"button\"\n              data-bs-target=\"#carouselExampleIndicators\"\n              data-bs-slide=\"prev\"\n              >\n        <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\n        <span class=\"visually-hidden\">Previous</span>\n      </button>\n      <button class=\"carousel-control-next\"\n              type=\"button\"\n              data-bs-target=\"#carouselExampleIndicators\"\n              data-bs-slide=\"next\"\n              >\n        <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\n        <span class=\"visually-hidden\">Next</span>\n      </button>\n      <div class=\"position-absolute top-50 start-50 translate-middle rounded-2\"\n           style=\"background-color: rgba(255, 255, 255, 0.6);\">\n        <div class=\"d-flex flex-column align-items-center p-2\">\n          <img class=\"mb-2\" src=\"~@/assets/img/BRITE Logo.png\" alt=\"BRITE\" width=\"200\" height=\"60\">\n          <h2>Light up the world, Brighter your life.</h2>\n        </div>\n      </div>\n    </div>\n  </header>\n  <!-- Latest Product List -->\n  <section class=\"mb-3\">\n    <ProductList\n      :allProducts=\"allProducts\" :status=\"pageInfo\" @clickProduct=\"handleProductClick\"\n      >\n    </ProductList>\n  </section>\n  <!-- Introductino of our service -->\n  <section>\n    <div class=\"container\">\n      <div class=\"row mb-5\">\n        <div class=\"col-md-6\">\n          <img class=\"h-100 w-100\" src=\"~@/assets/img/homepage/freeshipping.jpg\" alt=\"Temporary01\">\n        </div>\n        <div class=\"col-md-6 align-self-center\">\n          <h3 class=\"display-5 my-5\">Free Shipping</h3>\n          <p class=\"fs-5 lh-base\">\n            We offer free shipping on all orders, no minimum purchase required.\n            Enjoy the convenience of having your items delivered to your doorstep at no extra cost.\n            Whether it's everyday essentials or special treats,\n            we've got you covered with complimentary shipping on every order.\n            Shop now and experience hassle-free delivery!\n          </p>\n        </div>\n      </div>\n      <div class=\"row flex-md-row-reverse mb-5\">\n        <div class=\"col-md-6\">\n          <img class=\"h-100 w-100\" src=\"~@/assets/img/homepage/consulation.jpg\" alt=\"Temporary01\">\n        </div>\n        <div class=\"col-md-6 align-self-center\">\n          <h2 class=\"display-5 my-5\">Complimentary Consultation</h2>\n          <p class=\"fs-5 lh-base\">\n            Experience our complimentary consultation service where our expert team assists you\n            in selecting the perfect lighting fixtures to complement your indoor ambiance\n            or design a unique lighting solution tailored specifically to your needs.\n          </p>\n        </div>\n      </div>\n      <div class=\"row mb-5\">\n        <div class=\"col-md-6\">\n          <img class=\"h-100 w-100\" src=\"~@/assets/img/homepage/warranty.jpg\" alt=\"Temporary01\">\n        </div>\n        <div class=\"col-md-6 align-self-center\">\n          <h3 class=\"display-5 my-5\">3 Year Warranty</h3>\n          <p class=\"fs-5 lh-base\">\n            Our lighting fixtures come with a comprehensive 3-year warranty,\n            ensuring peace of mind and satisfaction with your purchase.\n            We stand behind the quality of our products and are committed\n            to providing reliable support should any issues arise during the warranty period.\n            With our 3-year warranty service,\n            you can trust in the durability and longevity of your lighting investment.\n          </p>\n        </div>\n      </div>\n    </div>\n  </section>\n  <!-- Go shopping now -->\n  <section>\n    <div class=\"container-fluid text-center\">\n      <h3 class=\"display-5 my-5\">Many products are available for you</h3>\n      <div class=\"row mb-5\">\n        <div class=\"col-md col-4 p-0 type-1\">\n          <a class=\"btn p-0 border-0 w-100 h-100 d-flex flex-column\"\n             @click=\"intoSelectedType('All')\"\n             >\n            <p class=\"text-white mb-0 mt-auto fs-3 py-1\"\n               style=\"background-color: rgba(0, 0, 0, 0.5);\">All</p>\n          </a>\n        </div>\n        <div class=\"col-md col-4 p-0 type-2\">\n          <a class=\"btn p-0 border-0 w-100 h-100 d-flex flex-column\"\n             @click=\"intoSelectedType('Ceiling')\"\n             >\n            <p class=\"text-white mb-0 mt-auto fs-3 py-1\"\n               style=\"background-color: rgba(0, 0, 0, 0.5);\">Ceiling</p>\n          </a>\n        </div>\n        <div class=\"col-md col-4 p-0 type-3\">\n          <a class=\"btn p-0 border-0 w-100 h-100 d-flex flex-column\"\n             @click=\"intoSelectedType('Clip')\"\n             >\n            <p class=\"text-white mb-0 mt-auto fs-3 py-1\"\n               style=\"background-color: rgba(0, 0, 0, 0.5);\">Clip</p>\n          </a>\n        </div>\n        <div class=\"col-md col-4 p-0 type-4\">\n          <a class=\"btn p-0 border-0 w-100 h-100 d-flex flex-column\"\n             @click=\"intoSelectedType('Floor')\"\n             >\n            <p class=\"text-white mb-0 mt-auto fs-3 py-1\"\n               style=\"background-color: rgba(0, 0, 0, 0.5);\">Floor</p>\n          </a>\n        </div>\n        <div class=\"col-md col-4 p-0 type-5\">\n          <a class=\"btn p-0 border-0 w-100 h-100 d-flex flex-column\"\n             @click=\"intoSelectedType('Pendant')\"\n             >\n            <p class=\"text-white mb-0 mt-auto fs-3 py-1\"\n               style=\"background-color: rgba(0, 0, 0, 0.5);\">Pendant</p>\n          </a>\n        </div>\n        <div class=\"col-md col-4 p-0 type-6\">\n          <a class=\"btn p-0 border-0 w-100 h-100 d-flex flex-column\"\n             @click=\"intoSelectedType('Table')\"\n             >\n            <p class=\"text-white mb-0 mt-auto fs-3 py-1\"\n               style=\"background-color: rgba(0, 0, 0, 0.5);\">Table</p>\n          </a>\n        </div>\n      </div>\n      <router-link to=\"/products\" role=\"button\"\n                   class=\"btn btn-primary btn-lg text-white mb-5\">\n        Go shopping now\n      </router-link>\n    </div>\n  </section>\n  <Footer></Footer>\n</template>\n\n<script>\nimport Navbar from '@/components/Navbar.vue';\nimport Footer from '@/components/Footer.vue';\nimport ProductList from '@/components/HorizontalProductList.vue';\n\nexport default {\n  components: {\n    Navbar,\n    Footer,\n    ProductList,\n  },\n  data() {\n    return {\n      showAd: false,\n      isLoading: false,\n      allProducts: [],\n      allProductTypes: [],\n      pageInfo: {\n        title: 'Latest',\n        type: 'All',\n        name: '',\n      },\n    };\n  },\n  methods: {\n    getProducts() {\n      const url = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/products/all`;\n      this.isLoading = true;\n      this.$http.get(url)\n        .then((res) => {\n          this.allProducts = res.data.products;\n          this.allProductTypes = ['All', ...new Set(this.allProducts.map((product) => product.category))];\n          this.isLoading = false;\n        });\n    },\n    closeAd() {\n      this.showAd = false;\n    },\n    intoSelectedType(type) {\n      // 導航到 Products.vue 並將字串作為參數傳遞\n      this.$router.push({\n        path: '/products',\n        query: { data: type },\n      });\n    },\n  },\n  // mounted() {\n  //   setTimeout(() => {\n  //     this.showAd = true;\n  //   }, 3000);\n  // },\n  created() {\n    this.getProducts();\n  },\n};\n</script>\n"],"mappings":";AAiNA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,WAAU,MAAO,wCAAwC;AAEhE,eAAe;EACbC,UAAU,EAAE;IACVH,MAAM;IACNC,MAAM;IACNC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,KAAK;MACbC,SAAS,EAAE,KAAK;MAChBC,WAAW,EAAE,EAAE;MACfC,eAAe,EAAE,EAAE;MACnBC,QAAQ,EAAE;QACRC,KAAK,EAAE,QAAQ;QACfC,IAAI,EAAE,KAAK;QACXC,IAAI,EAAE;MACR;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,MAAMC,GAAE,GAAK,GAAEC,OAAO,CAACC,GAAG,CAACC,WAAY,OAAMF,OAAO,CAACC,GAAG,CAACE,YAAa,eAAc;MACpF,IAAI,CAACb,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACc,KAAK,CAACC,GAAG,CAACN,GAAG,EACfO,IAAI,CAAEC,GAAG,IAAK;QACb,IAAI,CAAChB,WAAU,GAAIgB,GAAG,CAACnB,IAAI,CAACoB,QAAQ;QACpC,IAAI,CAAChB,eAAc,GAAI,CAAC,KAAK,EAAE,GAAG,IAAIiB,GAAG,CAAC,IAAI,CAAClB,WAAW,CAACmB,GAAG,CAAEC,OAAO,IAAKA,OAAO,CAACC,QAAQ,CAAC,CAAC,CAAC;QAC/F,IAAI,CAACtB,SAAQ,GAAI,KAAK;MACxB,CAAC,CAAC;IACN,CAAC;IACDuB,OAAOA,CAAA,EAAG;MACR,IAAI,CAACxB,MAAK,GAAI,KAAK;IACrB,CAAC;IACDyB,gBAAgBA,CAACnB,IAAI,EAAE;MACrB;MACA,IAAI,CAACoB,OAAO,CAACC,IAAI,CAAC;QAChBC,IAAI,EAAE,WAAW;QACjBC,KAAK,EAAE;UAAE9B,IAAI,EAAEO;QAAK;MACtB,CAAC,CAAC;IACJ;EACF,CAAC;EACD;EACA;EACA;EACA;EACA;EACAwB,OAAOA,CAAA,EAAG;IACR,IAAI,CAACrB,WAAW,CAAC,CAAC;EACpB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}