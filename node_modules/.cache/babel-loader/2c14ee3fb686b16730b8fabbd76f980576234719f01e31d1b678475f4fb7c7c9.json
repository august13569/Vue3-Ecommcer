{"ast":null,"code":"import Navbar from '@/components/Navbar.vue';\nimport Footer from '@/components/Footer.vue';\nimport ToastMessages from '@/components/ToastMessages.vue';\nimport { Field, Form } from 'vee-validate';\nexport default {\n  components: {\n    Navbar,\n    Footer,\n    Field,\n    Form,\n    ToastMessages\n  },\n  data() {\n    return {\n      form: {\n        user: {\n          name: '',\n          email: '',\n          tel: '',\n          message: ''\n        }\n      }\n    };\n  },\n  methods: {\n    sendMessgae() {\n      this.$emitter.emit('push-message', {\n        style: 'primary',\n        title: 'The message has been sent.'\n      });\n    }\n  }\n};","map":{"version":3,"names":["Navbar","Footer","ToastMessages","Field","Form","components","data","form","user","name","email","tel","message","methods","sendMessgae","$emitter","emit","style","title"],"sources":["C:\\Users\\augus\\OneDrive\\桌面\\暫存區\\vue-practice\\brite\\src\\views\\front\\Contact.vue"],"sourcesContent":["<!-- 要新增送出訊息的提示與邏輯 -->\n<template>\n<ToastMessages></ToastMessages>\n<Navbar></Navbar>\n<!-- Banner -->\n<header class=\"header-4 d-flex align-items-center justify-content-center mb-5\">\n  <div class=\"text-white text-center w-100\"\n         style=\"background-image: radial-gradient(circle, rgba(0, 0, 0, 0.5) 0%,\n                                rgba(0, 0, 0, 0) 100%);\">\n      <h2 class=\"display-1\">Contact</h2>\n    </div>\n</header>\n<!-- Our Contact Info -->\n<div class=\"container\">\n  <ul class=\"list-group list-group-horizontal\n             flex-column flex-md-row justify-content-md-around mb-5\"\n             >\n    <li class=\"list-group-item text-center border-0\">\n      <i class=\"bi bi-telephone-fill display-3\"></i>\n      <h3>Phone</h3>\n      <p>Phone: 02-6666-5200</p>\n      <p>Fax: 02-6666-5220</p>\n    </li>\n    <li class=\"list-group-item text-center border-0\">\n      <i class=\"bi bi-geo-alt-fill display-3\"></i>\n      <h3>Location</h3>\n      <p>11775 EAST 45TH AVENUE.</p>\n      <p>DENVER, CO 80239</p>\n    </li>\n    <li class=\"list-group-item text-center border-0\">\n      <i class=\"bi bi-envelope-fill display-3\"></i>\n      <h3>Email</h3>\n      <p>support@brite.com</p>\n    </li>\n  </ul>\n  <h3 class=\"display-5 text-center mb-5\">Send Us Message</h3>\n  <!-- Customers Contact Info -->\n  <Form class=\"row g-3\" v-slot=\"{ errors }\">\n    <div class=\"col-md-6\">\n      <div class=\"mb-3\">\n        <label for=\"name\" class=\"form-label\">Name</label>\n        <Field id=\"name\" name=\"name\" type=\"text\" class=\"form-control\"\n               :class=\"{ 'is-invalid': errors['name'] }\"\n               placeholder=\"Please enter your name\" rules=\"required\"\n               v-model=\"form.user.name\"></Field>\n        <ErrorMessage name=\"name\" class=\"invalid-feedback\"></ErrorMessage>\n      </div>\n      <div class=\"mb-3\">\n        <label for=\"email\" class=\"form-label\">Email</label>\n        <Field id=\"email\" name=\"email\" type=\"email\" class=\"form-control\"\n               :class=\"{ 'is-invalid': errors['email'] }\"\n               placeholder=\"Please enter your email\" rules=\"email|required\"\n               v-model=\"form.user.email\"></Field>\n        <ErrorMessage name=\"email\" class=\"invalid-feedback\"></ErrorMessage>\n      </div>\n      <div class=\"mb-3\">\n        <label for=\"tel\" class=\"form-label\">Phone</label>\n        <Field id=\"tel\" name=\"phone\" type=\"tel\" class=\"form-control\"\n               :class=\"{ 'is-invalid': errors['phone'] }\"\n               placeholder=\"Please enter your phone\" rules=\"required\"\n               v-model=\"form.user.tel\"></Field>\n        <ErrorMessage name=\"phone\" class=\"invalid-feedback\"></ErrorMessage>\n      </div>\n    </div>\n    <div class=\"col-md-6\">\n      <label for=\"message\" class=\"form-label\">Message</label>\n        <Field style=\"height: 80%; max-height: 80%;\"\n               id=\"message\" name=\"message\" as=\"textarea\" class=\"form-control\"\n               :class=\"{ 'is-invalid': errors['message'] }\"\n               v-model=\"form.user.message\"\n               placeholder=\"Please enter the message\"\n               rules=\"required\"\n               >\n        </Field>\n        <ErrorMessage name=\"message\" class=\"invalid-feedback\"></ErrorMessage>\n    </div>\n    <!-- 要新增送出訊息的提示與邏輯 -->\n    <div class=\"col-12 text-center mt-5\">\n      <button class=\"btn btn-primary btn-lg text-white mb-5\" type=\"submit\">Submit</button>\n    </div>\n  </Form>\n</div>\n<Footer></Footer>\n</template>\n\n<script>\nimport Navbar from '@/components/Navbar.vue';\nimport Footer from '@/components/Footer.vue';\nimport ToastMessages from '@/components/ToastMessages.vue';\nimport { Field, Form } from 'vee-validate';\n\nexport default {\n  components: {\n    Navbar,\n    Footer,\n    Field,\n    Form,\n    ToastMessages,\n  },\n  data() {\n    return {\n      form: {\n        user: {\n          name: '',\n          email: '',\n          tel: '',\n          message: '',\n        },\n      },\n    };\n  },\n  methods: {\n    sendMessgae() {\n      this.$emitter.emit('push-message', {\n        style: 'primary',\n        title: 'The message has been sent.',\n      });\n    },\n  },\n};\n</script>\n"],"mappings":"AAsFA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,aAAY,MAAO,gCAAgC;AAC1D,SAASC,KAAK,EAAEC,IAAG,QAAS,cAAc;AAE1C,eAAe;EACbC,UAAU,EAAE;IACVL,MAAM;IACNC,MAAM;IACNE,KAAK;IACLC,IAAI;IACJF;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,IAAI,EAAE;UACJC,IAAI,EAAE,EAAE;UACRC,KAAK,EAAE,EAAE;UACTC,GAAG,EAAE,EAAE;UACPC,OAAO,EAAE;QACX;MACF;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACC,QAAQ,CAACC,IAAI,CAAC,cAAc,EAAE;QACjCC,KAAK,EAAE,SAAS;QAChBC,KAAK,EAAE;MACT,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}